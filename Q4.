Scanner l = new Scanner(System.in);
      int[] numbre = {60, 70, 80, 90, 100, 160, 170, 180, 190, 200,};
      System.out.println("Enter your number:");
      int check = l.nextInt();
       for (int u:numbre){
          if (u == check) {
              System.out.println("The number is:" + u);
             break;
        }
      }
